{
  "name": "@statewalker/webrun-git",
  "version": "0.0.0",
  "description": "Git version management on top of the files API",
  "keywords": [],
  "homepage": "https://github.com/statewalker/webrun-git",
  "author": {
    "name": "Mikhail Kotelnikov",
    "email": "mikhail.kotelnikov@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/package.json",
    "dist/**/*.js",
    "src/**/*.js",
    "index.js"
  ],
  "module": "src/index.js",
  "main": "src/index.js",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "exports": {
    "umd": "./dist/index-umd.min.js",
    "default": "./src/index.js"
  },
  "dependencies": {
    "@statewalker/webrun-files": "0.0.0"
  },
  "devDependencies": {
    "@statewalker/rollup": "^0.1.5",
    "isomorphic-git": "^1.21.0",
    "eslint": "^8",
    "expect.js": "^0.3",
    "mocha": "^10",
    "rollup": "^3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:statewalker/webrun-git.git"
  },
  "scripts": {
    "eslint": "eslint src",
    "rollup": "rollup -c",
    "test": "rm -fr ./test/nginx-git && cp -r ./test/nginx-git-template ./test/nginx-git && docker-compose -f ./docker-compose-nginx-git.yml up -d && mocha -R spec ./test/index.js && docker-compose -f ./docker-compose-nginx-git.yml down && yarn eslint",
    "prepublishOnly": "rm -rf dist && yarn test && yarn rollup"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}